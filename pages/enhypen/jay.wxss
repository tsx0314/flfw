.table {
    display: flex;
    flex-wrap: wrap;
  }
  
  .table-cell {
    width: 33.33%;
    box-sizing: border-box;
    padding: 10px; /* Adjust padding as needed */
    border: 1px solid #ddd; /* Optional border */
  }
  
  .table-cell image {
    width: 100%;
    /* Set a fixed height or use aspectFit to control the image's aspect ratio */
  }
  
  .table-cell text {
    display: block;
    text-align: center;
    margin-bottom: 5px; /* Spacing between title and image */
  }
  